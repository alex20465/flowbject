{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "declaration": true,
    "target": "es5",
    "outDir": "dist"
  },
  "include": [
    "./src/**/*.ts",
    "./test/**/*.ts"
  ], 
  "exclude": [
    "./node_modules/**/*"
  ]
}